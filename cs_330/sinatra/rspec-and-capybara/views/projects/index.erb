<!doctype>
<html>
<head>
    <title>All Projects</title>
    <link rel="stylesheet" type="text/css" href="css/styles.css" />
</head>

<body>

<div id="wrapper">

  <h1>All Projects</h1>

  <ul>
    <% @projects.reverse.each do |p| %>
        <li><a href="/projects/<%= p.id %>"><%= p.name %><br /><%= p.description %></a></li>
    <% end %>
  </ul>

</div>

</body>
</html>